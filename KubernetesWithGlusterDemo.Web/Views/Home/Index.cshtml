@model IEnumerable<KeyValuePair<string, string>>
@{
    ViewData["Title"] = "Diagnostic Info";
}

<div class="text-center">
    <h1 class="display-4">Diagnostic Info</h1>
    <a asp-action="RemoveFile">Remove cache file</a>
    |
    <a asp-action="MakeUnhealthy">Remove healthok.txt</a>

        @foreach (var pair in Model)
        {
            <p>
                <b>@pair.Key</b>: @pair.Value
            </p>
        }
</div>
